<h1>Füdizzles</h1>

<a href="/articles/new" class="btn btn-primary">Unleash your Füdi!</a><br />

<div class="card text-center">
    <div class="card-header">
      Featured
    </div>
    <div class="card-body">
      <h5 class="card-title">Special title treatment</h5>
      <p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
      <a href="#" class="btn btn-primary">Go somewhere</a>
    </div>
    <div class="card-footer text-muted">
      2 days ago
    </div>
  </div>



<% articles.forEach(function(article, idx) { %>
  <div class="well">
    <a href="/articles/<%= article.id %>"><h2><%= article.title %></h2><h5>Written by: <%= article.author %></h5></a>
    <a class="btn btn-warning" href="/articles/<%= article.id %>/edit">Edit This Post</a>
    <a class="btn btn-danger delete" href="/articles/<%= article.id %>">Delete</a>
  </div>
<% }); %>
